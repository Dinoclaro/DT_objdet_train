<launch>
  <arg name="veh" doc="Name of vehicle. ex: megaman"/>
  <arg name="AIDO_eval" default="false"/>
  <arg name="param_file_name" default="default" doc="Specify a param file. ex:megaman." />

    <arg name="pkg_name" value="projection"/>
    <arg name="node_name" default="projection_node"/>

    <node ns="$(arg veh)" name="$(arg node_name)" pkg="$(arg pkg_name)" type="$(arg node_name).py" output="screen">
        <rosparam command="load" file="$(find projection)/config/$(arg node_name)/$(arg param_file_name).yaml"/>
        <param name="AIDO_eval" value="$(arg AIDO_eval)"/>
    </node>
</launch>